   "source": [
    "def volts_to_code(v: float) -> int:\n",
    "\n",
    "    max_v = 5.0\n",
    "    if not (0.0 <= v <= max_v):\n",
    "        raise ValueError(\"Voltage must be between 0 V and 5 V\")\n",
    "\n",
    "    code = round(v / max_v * 0xFFFF)\n",
    "    return int(code)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "6c1925c4",
